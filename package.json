{
  "name": "n8n-nodes-dav",
  "version": "0.1.5",
  "description": "Custom n8n nodes for WebDAV, CalDAV, and CardDAV protocols",
  "keywords": [
    "n8n-community-node-package",
    "webdav",
    "caldav",
    "carddav",
    "dav"
  ],
  "license": "MIT",
  "homepage": "https://github.com/x40x1/n8n-nodes-dav",
  "author": {
    "name": "x40x1",
    "email": "alexanderschuetz@ymail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/x40x1/n8n-nodes-dav.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "smoke:webdav": "node ./scripts/webdav-smoke.mjs"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/DavApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/WebDav/WebDav.node.js",
      "dist/nodes/CalDav/CalDav.node.js",
      "dist/nodes/CardDav/CardDav.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "jest": "^30.1.3",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.1",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
